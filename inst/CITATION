citHeader("To cite dent in publications use:")

bibentry(
   bibtype = "Manual",
   title = "dent: Design of equivalence and noninferiority tests",
   author = c(person("Anonymous", "Author")),
   year = 2023,
   note = "R package version 0.0.1",
   url = "https://github.com/github-anonymous-author/dent")